{
  "name": "vitest-browser-commands",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@10.24.0",
  "description": "A set of custom server commands for Vitest browser mode",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/vitest-browser-commands#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/vitest-browser-commands.git"
  },
  "bugs": "https://github.com/ocavue/vitest-browser-commands/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./playwright": {
      "types": "./dist/playwright.d.ts",
      "import": "./dist/playwright.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "playwright": [
        "./dist/playwright.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "fix": "eslint --fix . && prettier --write .",
    "test": "vitest",
    "typecheck": "tsc -b",
    "prepare:playwright": "playwright install"
  },
  "peerDependencies": {
    "@vitest/browser-playwright": "^4.0.3",
    "vitest": "^4.0.3"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    },
    "@vitest/browser-playwright": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ocavue/eslint-config": "^3.7.1",
    "@ocavue/tsconfig": "^0.6.2",
    "@types/node": "^20.19.10",
    "@vitest/browser-playwright": "^4.0.15",
    "eslint": "^9.39.1",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "tsdown": "^0.17.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  }
}
